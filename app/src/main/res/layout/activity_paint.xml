<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:theme="@style/Theme.Design.Light">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:theme="@style/Theme.Design.Light">

        <one.empty3.feature.app.maxSdk29.pro.ImageViewSelection
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:contentDescription="@string/preview_of_shapes"
            android:theme="@style/Theme.Design.Light">

        </one.empty3.feature.app.maxSdk29.pro.ImageViewSelection>

        <Toolbar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <androidx.appcompat.widget.ActionMenuView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <ImageButton
                    android:src="@color/material_dynamic_primary70"
                    android:contentDescription="@string/primary_pencil_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </ImageButton>
            </androidx.appcompat.widget.ActionMenuView>
        </Toolbar>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>