<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        >

        <one.empty3.feature.app.maxSdk29.pro.ImageViewSelection
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:autofillHints="photo" />

        <Button
            android:id="@+id/backToMain"
            android:layout_height="40dp"
            android:layout_width="match_parent"
            android:hint="@string/go_to_previous_activity" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_constraintVertical_weight="0.125"
            android:text="@string/p_x_y_color" />

        <EditText
            android:id="@+id/x"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_constraintVertical_weight="0.125"
            android:autofillHints="X line">

        </EditText>

        <EditText
            android:id="@+id/y"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_constraintVertical_weight="0.125"
            android:autofillHints="Y column">

        </EditText>

        <EditText
            android:id="@+id/z"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_constraintVertical_weight="0.125"
            android:autofillHints="Z deep">

        </EditText>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>