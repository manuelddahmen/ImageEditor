<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />

    <application
        android:exported="false"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:hardwareAccelerated="true"
        android:theme="@style/Theme.FeatureAppmain"
        android:usesCleartextTraffic="false">
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/title_activity_main" />
        <activity
            android:name=".MainActivityComposeV2"
            android:exported="false"
            android:label="@string/title_activity_main_compose_v2" />

        <activity
            android:name=".TextActivity"
            android:exported="false"
            android:label="@string/title_activity_text"
            android:theme="@style/Theme.FeatureAppmain"
            android:hardwareAccelerated="true"

            />
        <activity
            android:name=".ChooseEffectsCompose"
            android:exported="false"
            android:label="@string/title_activity_choose_effects_compose"
            android:theme="@style/Theme.FeatureAppmain"

            />
        <activity
            android:name=".ChooseEffectsActivity2"
            android:exported="false"
            android:label="@string/title_activity_choose_effect_list"
            android:theme="@style/Theme.FeatureAppmain"
            android:hardwareAccelerated="true">
            <intent-filter>
                <action android:name="android.media.action.IMAGE_CAPTURE" />
                <action android:name="android.media.action.STILL_IMAGE_CAMERA" />
                <action android:name="android.media.action.VIDEO_CAMERA" />
                <action android:name="android.intent.action.EDIT" />
                <action android:name="android.intent.action.CREATE_DOCUMENT" />
                <!-- <action android:name="android.intent.action.MAIN" /> -->


                <!-- Default for launch requests -->
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>

        </activity>
        <activity
            android:name=".MyCameraActivity"
            android:hardwareAccelerated="true"
            android:exported="false"
            android:theme="@style/Theme.FeatureAppmain"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="image/*" />
            </intent-filter>
            <!-- Register as a system camera app -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity> <!-- FileProvider used to share photos with other apps -->
        <activity
            android:name=".LicenceUserData"
            android:exported="false"
            android:theme="@style/Theme.FeatureAppmain"
            android:hardwareAccelerated="true"

            />
        <activity
            android:name=".FormulaStore"
            android:exported="false"
            android:label="@string/title_activity_formula_store"
            android:theme="@style/Theme.FeatureAppmain"
            android:hardwareAccelerated="true"

            />
        <activity
            android:name=".LicenceUserData2"
            android:exported="false"
            android:theme="@style/Theme.FeatureAppmain"
            android:hardwareAccelerated="true"

            /> <!-- <a href="https://www.flaticon.com/free-icons/electronics" title="electronics icons">Electronics icons created by 88 Cloud - Flaticon</a> -->
        <activity
            android:name=".Calculator"
            android:exported="false"
            android:label="Simple Calculator"
            android:theme="@style/Theme.FeatureAppmain"
            android:hardwareAccelerated="true"

            />
        <activity
            android:name=".GraphicsActivity"
            android:exported="false"
            android:label="Draw graphics"
            android:theme="@style/Theme.FeatureAppmain"
            android:hardwareAccelerated="true"

            />
        <activity
            android:name=".GraphicsActivityView"
            android:exported="false"
            android:label="Draw graphics"
            android:hardwareAccelerated="true"
            android:theme="@style/Theme.FeatureAppmain"

            />

        <provider
            android:name=".MyFileProvider"
            android:authorities="one.empty3.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true" />
        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>

</manifest>